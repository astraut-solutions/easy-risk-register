{
  "name": "easy-risk-register-root",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cd easy-risk-register-frontend && npm run dev",
    "dev:docker": "docker-compose --profile development up --build frontend-dev",
    "prod:docker": "docker-compose up --build",
    "install": "cd easy-risk-register-frontend && npm install --include=dev",
    "check:no-vite-secrets": "node scripts/check-no-vite-secrets.js",
    "build": "npm run check:no-vite-secrets && cd easy-risk-register-frontend && npm run build",
    "setup": "npm run install && echo 'Setup complete! Run \"npm run dev\" to start the development server.'",
    "audit": "npm outdated && npm audit --omit=dev",
    "install:hooks": "node scripts/install-git-hooks.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
